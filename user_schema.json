{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User",
  "type": "object",
  "properties": {
    "birthday": {
      "description": "User's birthday as a Firestore timestamp.",
      "type": "string",
      "format": "date-time"
    },
    "created_time": {
      "description": "The time when the user was created in Firestore.",
      "type": "string",
      "format": "date-time"
    },
    "display_name": {
      "description": "User's display name.",
      "type": "string"
    },
    "email": {
      "description": "User's email address.",
      "type": "string",
      "format": "email"
    },
    "photo_url": {
      "description": "URL of the user's profile photo.",
      "type": "string",
      "format": "uri"
    },
    "uid": {
      "description": "Unique identifier for the user in Firestore.",
      "type": "string"
    }
  },
  "required": ["birthday", "created_time", "display_name", "email", "uid"],
  "additionalProperties": false
}
